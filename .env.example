# Environment Configuration Template
# Copy this file to .env.development, .env.staging, or .env.production

# Application
NODE_ENV=development
PORT=3001

# Client Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_WS_URL=ws://localhost:3001

# Database
REDIS_URL=redis://localhost:6379

# Mediasoup Configuration
MEDIASOUP_LISTEN_IP=0.0.0.0
MEDIASOUP_ANNOUNCED_IP=127.0.0.1
MEDIASOUP_MIN_PORT=40000
MEDIASOUP_MAX_PORT=40100

# WebRTC Configuration
STUN_SERVERS=stun:stun.l.google.com:19302
TURN_SERVER_URL=
TURN_SERVER_USERNAME=
TURN_SERVER_CREDENTIAL=

# OpenFace Configuration
OPENFACE_PATH=/usr/local/bin/OpenFace
OPENFACE_MODEL_PATH=/usr/local/share/OpenFace/model

# Audio AI Configuration
PYTHON_PATH=/usr/bin/python3
AUDIO_MODEL_PATH=./models/audio_emotion_model.h5

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log

# Security
JWT_SECRET=your-jwt-secret-here
CORS_ORIGIN=http://localhost:3000

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001